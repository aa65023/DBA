CREATE TABLE "VOIP_PORTAL"."BULK_LOAD_FILE_STATUS" 
   (	"REQUEST_ID" NUMBER NOT NULL,
	"BULK_LOAD_TYPE_ID" NUMBER NOT NULL,   
	"TENANT_ID" VARCHAR2(50 BYTE) NOT NULL,
    "GROUP_ID" VARCHAR2(50 BYTE),
	"NOTIFICATION_EMAILS" VARCHAR2(512 BYTE) NOT NULL,
	"INPUT_FILE_NAME" VARCHAR2(100 BYTE) NOT NULL,
	"INPUT_FILE_PATH" VARCHAR2(512 BYTE) NOT NULL,
	"INPUT_FILE_TYPE" NUMBER NOT NULL,
	"OUTPUT_FILE_NAME" VARCHAR2(100 BYTE),
	"OUTPUT_FILE_PATH" VARCHAR2(512 BYTE),
	"OUTPUT_FILE_TYPE" NUMBER,
	"CREATE_USER" VARCHAR2(50 BYTE) NOT NULL,
	"CREATE_DATE" DATE NOT NULL,
	"MOD_USER" VARCHAR2(50 BYTE),
	"MOD_DATE" DATE,
	"NOTIFICATION_DATE" DATE,
	"NOTIFICATION_RETRY_COUNT" NUMBER,
	"STATUS_CD" NUMBER NOT NULL,
	"STATUS_MSG" VARCHAR2(50),
	"COMMENTS" VARCHAR2(512 BYTE),
    "END_DATE" DATE,
	 CONSTRAINT "REQUEST_ID_PK" PRIMARY KEY ("REQUEST_ID"),
     CONSTRAINT "INPUT_FILE_NAME_UQ" UNIQUE ("INPUT_FILE_NAME"),
     CONSTRAINT "OUTPUT_FILE_NAME_UQ" UNIQUE ("OUTPUT_FILE_NAME")
   );

CREATE SEQUENCE  "VOIP_PORTAL"."REQUEST_ID_SEQ"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 61 CACHE 20 NOORDER  NOCYCLE  NOPARTITION ;

CREATE TABLE "VOIP_PORTAL"."BULK_LOAD_TN_DETAILS"
   (	"TN_DETAILS_ID" NUMBER NOT NULL,
	"REQUEST_ID" NUMBER NOT NULL,
	"TN" VARCHAR2(10 BYTE) NOT NULL,
	"PIN" VARCHAR2(50 BYTE) NOT NULL,
	"ANNOUNCEMENT_ID" VARCHAR2(100 BYTE) NOT NULL,
	"RECORDING_TYPE" VARCHAR2(50 BYTE) NOT NULL,
	"STATUS_CD" NUMBER NOT NULL,
	"RETRY_COUNT" NUMBER,
	"CREATE_USER" VARCHAR2(50 BYTE) NOT NULL,
	"CREATE_DATE" DATE NOT NULL,
	"MOD_USER" VARCHAR2(50 BYTE),
	"MOD_DATE" DATE,
    "COMMENTS" VARCHAR2(512 BYTE),
	"END_DATE" DATE,
	 PRIMARY KEY ("TN_DETAILS_ID"),
	 FOREIGN KEY ("REQUEST_ID")
	  REFERENCES "VOIP_PORTAL"."BULK_LOAD_FILE_STATUS" ("REQUEST_ID") ENABLE
   );

CREATE SEQUENCE  "VOIP_PORTAL"."TN_DETAILS_ID_SEQ"  MINVALUE 1 MAXVALUE 999999999999999999999999999 INCREMENT BY 1 START WITH 61 CACHE 20 NOORDER  NOCYCLE  NOPARTITION;

 CREATE TABLE "VOIP_PORTAL"."BULK_LOAD_TYPE"
 (
  "TYPE_ID" NUMBER NOT NULL,
  "VALUE" VARCHAR2(64 BYTE),
  CONSTRAINT "BULK_LOAD_TYPE_PK" PRIMARY KEY ("TYPE_ID")
  );

   CREATE TABLE "VOIP_PORTAL"."BULK_LOAD_FILE_TYPE"
  (
   "TYPE_ID" NUMBER NOT NULL ,
   "VALUE" VARCHAR2(64 BYTE) ,
   CONSTRAINT "FILE_TYPE_PK" PRIMARY KEY ("TYPE_ID")
   );

   create or replace synonym VOIP_PORTAL_APP.BULK_LOAD_TYPE for VOIP_PORTAL.BULK_LOAD_TYPE;
   create or replace synonym VOIP_PORTAL_APP.BULK_LOAD_FILE_TYPE for VOIP_PORTAL.BULK_LOAD_FILE_TYPE;
   create or replace synonym VOIP_PORTAL_APP.BULK_LOAD_FILE_STATUS for VOIP_PORTAL.BULK_LOAD_FILE_STATUS;
   create or replace synonym VOIP_PORTAL_APP.BULK_LOAD_TN_DETAILS for VOIP_PORTAL.BULK_LOAD_TN_DETAILS;



